int power(int n, int k) { // n^k
    int r;
    r = 1;
    while (k > 0) {
        if (k % 2 == 1) {
            r = r * n;
        } else {
        }
        n = n * n;
        k = k / 2;
    }
    return r;
}

void main() {
    int n;
    int k;
    read(n);
    read(k);
    print(power(n, k));
}

