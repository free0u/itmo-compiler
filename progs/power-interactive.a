int callCounter;

int power(int n, int k) { // n^k
    callCounter = callCounter + 1;
    int r;
    r = 1;
    while (k > 0) {
        if (k % 2 == 1) {
            r = r * n;
        } else {
        }
        n = n * n;
        k = k / 2;
    }
    return r;
}

